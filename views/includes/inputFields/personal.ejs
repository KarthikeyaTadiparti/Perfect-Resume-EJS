<div id="accordion-collapse-1" data-accordion="collapse" class="mt-5">
    <h2 id="accordion-collapse-heading-1">
        <button
            type="button"
            class="z-1 flex items-center justify-between bg-white w-full p-5 font-medium rtl:text-right text-gray-800 border border-gray-200 rounded-xl hover:bg-gray-100 gap-3"
            data-accordion-target="#accordion-collapse-body-1"
            aria-expanded="true"
            aria-controls="accordion-collapse-body-1"
        >
            <span>Personal Information</span>
            <svg
                data-accordion-icon
                class="w-3 h-3 rotate-180 shrink-0"
                aria-hidden="true"
                xmlns="http://www.w3.org/2000/svg"
                fill="none"
                viewBox="0 0 10 6"
            >
                <path
                    stroke="currentColor"
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    stroke-width="2"
                    d="M9 5 5 1 1 5"
                />
            </svg>
        </button>
    </h2>
    <div
        id="accordion-collapse-body-1"
        class="hidden relative -top-3"
        aria-labelledby="accordion-collapse-heading-1"
    >
        <div class="p-5 border border-t-0 bg-white border-gray-200">
            <!-- First and Last Name -->
            <div class="flex gap-4">
                <div class="w-full max-w-sm min-w-[200px] mt-4">
                    <label
                        for="firstName"
                        class="block mb-2 text-sm text-slate-600"
                    >
                        First Name
                    </label>
                    <input
                        type="text"
                        class="inputFields"
                        name="firstName"
                        id="firstName"
                        oninput="updatePreview()"
                    />
                </div>
                <div class="w-full max-w-sm min-w-[200px] mt-4">
                    <label
                        for="lastName"
                        class="block mb-2 text-sm text-slate-600"
                    >
                        Last Name
                    </label>
                    <input
                        type="text"
                        class="inputFields"
                        name="lastName"
                        id="lastName"
                        oninput="updatePreview()"
                    />
                </div>
            </div>

            <!-- Email and Mobile -->
            <div class="flex gap-4">
                <div class="grow mt-2">
                    <label for="email" class="block mb-2 text-sm text-slate-600">
                        Email Address
                    </label>
                    <input
                        type="text"
                        name="email"
                        id="email"
                        class="inputFields"
                        oninput="updatePreview()"
                    />
                </div>
                <div class="w-[200px] mt-2">
                    <label
                        for="number"
                        class="block mb-2 text-sm text-slate-600"
                    >
                        Mobile Number
                    </label>
                    <input
                        type="text"
                        name="number"
                        id="number"
                        class="inputFields"
                        oninput="updatePreview()"
                    />
                </div>
            </div>

            <!-- Address -->
            <div class="flex gap-4">
                <div class="w-full max-w-sm min-w-[200px] mt-2">
                    <label
                        for="address"
                        class="block mb-2 text-sm text-slate-600"
                    >
                        Address
                    </label>
                    <input
                        type="text"
                        name="city"
                        id="city"
                        placeholder="Enter your city"
                        class="inputFields"
                        oninput="updatePreview()"
                    />
                </div>
                <div class="w-full max-w-sm min-w-[200px] mt-9">
                    <input
                        type="text"
                        name="state"
                        id="state"
                        placeholder="Enter your state"
                        class="inputFields"
                        oninput="updatePreview()"
                    />
                </div>
            </div>

            <!-- Objective -->
            <label
                for="message"
                class="block my-2 text-sm text-slate-600"
                >Your Objective</label
            >
            <textarea
                id="message"
                rows="4"
                class="block p-2.5 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 focus:ring-blue-500 focus:border-blue-500"
                placeholder="Write your objective here..."
                oninput="updatePreview()"
            ></textarea>
        </div>
    </div>
</div>
